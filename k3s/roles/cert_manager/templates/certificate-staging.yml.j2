---
apiVersion: cert-manager.io/v1
kind: Certificate
metadata:
  name: certificate-staging
  namespace: default
spec:
  secretName: staging-tls
  issuerRef:
    name: letsencrypt-staging
    kind: ClusterIssuer
  commonName: "{{ k3s_certmanager.common_name }}"
  dnsNames: "{{ k3s_certmanager.dns_names }}"
