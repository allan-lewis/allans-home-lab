apiVersion: v1
kind: Service
metadata:
  name: longhorn-metallb
  namespace: longhorn-system
spec:
  internalTrafficPolicy: Cluster
  ipFamilies:
  - IPv4
  ipFamilyPolicy: SingleStack
  ports:
  - name: http
    port: 80
    protocol: TCP
    targetPort: http
  selector:
    app: longhorn-ui
  type: LoadBalancer
