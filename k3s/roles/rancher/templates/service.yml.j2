apiVersion: v1
kind: Service
metadata:
  name: rancher-metallb
  namespace: cattle-system
spec:
  ipFamilyPolicy: PreferDualStack
  ports:
  - name: http
    port: 80
    protocol: TCP
    targetPort: 80
  - name: https-internal
    port: 443
    protocol: TCP
    targetPort: 444
  selector:
    app: rancher
  type: LoadBalancer